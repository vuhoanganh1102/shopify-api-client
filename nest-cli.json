{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "shopify-api-client": {
      "type": "library",
      "root": "libs/shopify-api-client",
      "entryFile": "index",
      "sourceRoot": "libs/shopify-api-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shopify-api-client/tsconfig.lib.json"
      }
    },
    "mongo": {
      "type": "library",
      "root": "libs/mongo",
      "entryFile": "index",
      "sourceRoot": "libs/mongo/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongo/tsconfig.lib.json"
      }
    },
    "facebook": {
      "type": "library",
      "root": "libs/facebook",
      "entryFile": "index",
      "sourceRoot": "libs/facebook/src",
      "compilerOptions": {
        "tsConfigPath": "libs/facebook/tsconfig.lib.json"
      }
    }
  }
}