{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "shopify-api-client": {
      "type": "library",
      "root": "libs/shopify-api-client",
      "entryFile": "index",
      "sourceRoot": "libs/shopify-api-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shopify-api-client/tsconfig.lib.json"
      }
    },
    "mongo": {
      "type": "library",
      "root": "libs/mongo",
      "entryFile": "index",
      "sourceRoot": "libs/mongo/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongo/tsconfig.lib.json"
      }
    },
    "facebook": {
      "type": "library",
      "root": "libs/facebook",
      "entryFile": "index",
      "sourceRoot": "libs/facebook/src",
      "compilerOptions": {
        "tsConfigPath": "libs/facebook/tsconfig.lib.json"
      }
    },
    "schedules": {
      "type": "library",
      "root": "libs/schedules",
      "entryFile": "index",
      "sourceRoot": "libs/schedules/src",
      "compilerOptions": {
        "tsConfigPath": "libs/schedules/tsconfig.lib.json"
      }
    },
    "queues": {
      "type": "library",
      "root": "libs/queues",
      "entryFile": "index",
      "sourceRoot": "libs/queues/src",
      "compilerOptions": {
        "tsConfigPath": "libs/queues/tsconfig.lib.json"
      }
    },
    "caching": {
      "type": "library",
      "root": "libs/caching",
      "entryFile": "index",
      "sourceRoot": "libs/caching/src",
      "compilerOptions": {
        "tsConfigPath": "libs/caching/tsconfig.lib.json"
      }
    },
    "helper": {
      "type": "library",
      "root": "libs/helper",
      "entryFile": "index",
      "sourceRoot": "libs/helper/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helper/tsconfig.lib.json"
      }
    },
    "mysql": {
      "type": "library",
      "root": "libs/mysql",
      "entryFile": "index",
      "sourceRoot": "libs/mysql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mysql/tsconfig.lib.json"
      }
    }
  }
}