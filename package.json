{
  "name": "shoipfy-api-client",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./libs/mysql/src/dataSource.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create ./libs/mysql/src/migrations/migrations",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./libs/mysql/src/migrations/$npm_config_name -d ./libs/mysql/src/dataSource.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./libs/mysql/src/dataSource.ts"
  },
  "dependencies": {
    "@keyv/redis": "^4.3.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@shopify/admin-api-client": "^1.0.6",
    "@shopify/shopify-api": "^11.9.0",
    "@shopify/shopify-app-express": "^5.0.14",
    "@shopify/shopify-app-session-storage-mongodb": "^4.0.13",
    "@shopify/shopify-app-session-storage-mysql": "^4.0.14",
    "axios": "^1.8.1",
    "bullmq": "^5.41.7",
    "cache-manager": "^6.4.1",
    "cacheable": "^1.8.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.1",
    "moment-timezone": "^0.5.47",
    "mongoose": "^8.12.1",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/shopify-api-client(|/.*)$": "<rootDir>/libs/shopify-api-client/src/$1",
      "^@app/mongo(|/.*)$": "<rootDir>/libs/mongo/src/$1",
      "^@app/facebook(|/.*)$": "<rootDir>/libs/facebook/src/$1",
      "^@app/schedules(|/.*)$": "<rootDir>/libs/schedules/src/$1",
      "^@app/queues(|/.*)$": "<rootDir>/libs/queues/src/$1",
      "^@app/caching(|/.*)$": "<rootDir>/libs/caching/src/$1",
      "^@app/helper(|/.*)$": "<rootDir>/libs/helper/src/$1",
      "^@app/mysql(|/.*)$": "<rootDir>/libs/mysql/src/$1"
    }
  }
}
